<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<services>
			<service name="VP_Baterija.BatteryService">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4000"/>
					</baseAddresses>
				</host>
				<!-- Service Endpoints -->
				<endpoint
				address="ServiceBattery"
				binding="netTcpBinding"
				contract="Common.Services.IBatteryAnalysisService"/>
			</service>
		</services>
	</system.serviceModel>
</configuration>
